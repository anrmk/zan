{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.js",
        "jquery.min.js"
      ]
    },

    {
      "library": "aspnet-signalr@1.1.4",
      "destination": "wwwroot/lib/signalr/"
    },
    {
      "library": "semantic-ui@2.4.1",
      "destination": "wwwroot/lib/semantic-ui/"
    },
    {
      "library": "semantic-ui-calendar@0.0.8",
      "destination": "wwwroot/lib/semantic-ui-calendar/"
    },
    {
      "provider": "filesystem",
      "library": "https://cdn.datatables.net/v/se/jq-3.3.1/dt-1.10.20/",
      "destination": "wwwroot/lib/jquery-datatables/",
      "files": [
        "datatables.min.css",
        "datatables.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "jquery-serializejson@2.9.0",
      "destination": "wwwroot/lib/jquery-serializejson/"
    }
  ]
}